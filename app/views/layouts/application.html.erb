<!DOCTYPE html>
<html>
<head>
  <title>Dopederp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
  <div class="row">
    <div class="col-sm-12">
  <%= render 'layouts/navbar' %>
    </div>
  </div>
  <div class="row">
    <div class="col-sm=12">
  	<% flash.each do |key, value| %>
  		<%= value %>
  	<% end %>

    <%= yield %>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-12">

      <footer>
      </footer>

    </div>
  </div>
</div>

</body>
</html>
