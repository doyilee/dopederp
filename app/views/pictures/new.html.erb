<%= form_for @picture, {url: pictures_path, multipart: true} do |f| %>
<br><br>
<%= f.label :title %>
<%= f.text_field :title %><br>
<%= f.label('Description: ') %> <br>
<%= f.text_area(:description) %><br>
<%= f.file_field(:photo) %>

<%= f.hidden_field :user_id, :value => session[:user_id]  %>
<br><br>
<%= f.submit('Upload photo!') %>

<% end %>
